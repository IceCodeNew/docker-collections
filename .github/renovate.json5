{
  "timezone": "Asia/Shanghai",
  "extends": [
    "schedule:earlyMondays",
    "config:recommended",
    // "docker:pinDigests",
    "helpers:pinGitHubActionDigests",
    ":configMigration",
    ":pinDevDependencies",
    "security:openssf-scorecard",
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^\\.markitdown_version$/"],
      "matchStrings": ["(?<currentValue>.*?)\\n"],
      "depNameTemplate": "markitdown",
      "datasourceTemplate": "pypi",
    },
  ],
}
